<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.GridPane?>


<GridPane fx:id="playerGameGridPane" hgap="10.0" vgap="10.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="controllers.DisplayPlayerGameController">
   <children>
      <Label text="Player Game Report" textAlignment="CENTER" GridPane.columnSpan="2147483647">
         <font>
            <Font name="System Bold Italic" size="20.0" />
         </font>
      </Label>
      <Label text="Select Player:" GridPane.rowIndex="1">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <ComboBox fx:id="selectPlayerComboBox" onAction="#selectPlayerComboBoxHandler" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Label fx:id="playerNameLabel" text="Games played by" GridPane.columnSpan="2147483647" GridPane.rowIndex="2">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Button fx:id="backButton" alignment="CENTER" mnemonicParsing="false" onAction="#backButtonHandler" text="Back" GridPane.rowIndex="3" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</GridPane>
